  <ui:overlay width="235" height="145">
  <media type="video" its:translate="no" src="figures/gnome-change-wallpaper.webm" width="700" height="394">
    <ui:thumb type="image" mime="image/svg" src="gs-thumb-changing-wallpaper.svg">
      <ui:caption its:translate="yes"><desc style="center">Vaihda taustakuva</desc></ui:caption>
    </ui:thumb>
      <tt:tt xmlns:tt="http://www.w3.org/ns/ttml" its:translate="yes">
       <tt:body>
         <tt:div begin="1s" end="5s">
           <tt:p>Taustakuvan vaihtaminen</tt:p>
         </tt:div>
         <tt:div begin="6s" end="9s">
           <tt:p>Avaa järjestelmävalikko yläpalkin oikeasta laidasta ja napsauta Asetukset-kuvaketta.</tt:p>
           </tt:div>
         <tt:div begin="10s" end="12s">
           <tt:p>Valitse <gui>Taustakuva</gui>.</tt:p>
         </tt:div>
         <tt:div begin="12s" end="13s">
           <tt:p>Napsauta nykyistä taustakuvaa.</tt:p>
         </tt:div>
         <tt:div begin="13s" end="16s">
           <tt:p>Napsauta kuvaa, jota haluat käyttää taustakuvana.</tt:p>
         </tt:div>
         <tt:div begin="16s" end="18s">
           <tt:p>Napsauta <gui>Valitse</gui>.</tt:p>
         </tt:div>
         <tt:div begin="18s" end="21s">
           <tt:p>Sulje <gui>Taustakuva</gui>-ikkuna.</tt:p>
