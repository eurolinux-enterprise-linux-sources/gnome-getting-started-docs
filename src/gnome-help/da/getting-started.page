<if:when test="platform:gnome-classic">

  <ui:overlay width="235" height="145">
  <media type="video" its:translate="no" src="figures/gnome-change-wallpaper.webm" width="700" height="394">
    <ui:thumb type="image" mime="image/svg" src="gs-thumb-changing-wallpaper.svg">
      <ui:caption its:translate="yes"><desc style="center">Skift baggrundstapetet</desc></ui:caption>
    </ui:thumb>
      <tt:tt xmlns:tt="http://www.w3.org/ns/ttml" its:translate="yes">
       <tt:body>
         <tt:div begin="1s" end="5s">
           <tt:p>Skifte baggrundstapet</tt:p>
         </tt:div>
         <tt:div begin="6s" end="9s">
           <tt:p>Klik på systemmenuen på højre side af toplinjen, og tryk på knappen indstillinger.</tt:p>
           </tt:div>
         <tt:div begin="10s" end="12s">
           <tt:p>Vælg <gui>Baggrund</gui>.</tt:p>
         </tt:div>
         <tt:div begin="12s" end="13s">
           <tt:p>Klik på det nuværende baggrundsbillede.</tt:p>
         </tt:div>
         <tt:div begin="13s" end="16s">
           <tt:p>Klik på det baggrundsbillede du vil bruge.</tt:p>
         </tt:div>
         <tt:div begin="16s" end="18s">
           <tt:p>Klik på knappen <gui>Vælg</gui>.</tt:p>
         </tt:div>
         <tt:div begin="18s" end="21s">
           <tt:p>Luk vinduet <gui>Baggrund</gui>.</tt:p>
         </tt:div>
