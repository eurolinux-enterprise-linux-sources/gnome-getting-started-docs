<if:when test="platform:gnome-classic">

  <ui:overlay width="235" height="145">
  <media type="video" its:translate="no" src="figures/gnome-change-wallpaper.webm" width="700" height="394">
    <ui:thumb type="image" mime="image/svg" src="gs-thumb-changing-wallpaper.svg">
      <ui:caption its:translate="yes"><desc style="center">Promena pozadine</desc></ui:caption>
    </ui:thumb>
      <tt:tt xmlns:tt="http://www.w3.org/ns/ttml" its:translate="yes">
       <tt:body>
         <tt:div begin="1s" end="5s">
           <tt:p>Promena pozadine</tt:p>
         </tt:div>
         <tt:div begin="6s" end="9s">
           <tt:p>Kliknite na sistemski izbornik u desnoj strani gornje trake i pritisnite dugme za podešavanja.</tt:p>
           </tt:div>
         <tt:div begin="10s" end="12s">
           <tt:p>Izaberite <gui>Pozadina</gui>.</tt:p>
         </tt:div>
         <tt:div begin="12s" end="13s">
           <tt:p>Kliknite na trenutnu sliku pozadine.</tt:p>
         </tt:div>
         <tt:div begin="13s" end="16s">
           <tt:p>Kliknite na sliku koju želite da koristite kao pozadinu.</tt:p>
         </tt:div>
         <tt:div begin="16s" end="18s">
           <tt:p>Kliknite na dugme <gui>Izaberi</gui>.</tt:p>
         </tt:div>
         <tt:div begin="18s" end="21s">
           <tt:p>Zatvorite prozor <gui>Pozadina</gui>.</tt:p>
         </tt:div>
